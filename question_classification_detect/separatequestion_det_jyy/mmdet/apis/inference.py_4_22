    if(index_paper.shape[0] > 1):
        for _i in range(num):
            i = order[_i]
            if suppressed[i] == 1:# 对于抑制的检测框直接跳过
                continue
            keep_index.append(i)# 保留当前框的索引
            if(not i in index_paper.tolist()):
                continue

            xx1 = np.maximum(x1[i], x1[index_paper])
            yy1 = np.maximum(y1[i], y1[index_paper])
